<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment">

    <com.txusballesteros.widgets.FitChart
        android:id="@+id/fitChart"
        android:layout_width="248dp"
        android:layout_height="287dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="132dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeSize="30dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="17 days!"
        android:textColor="#000000"
        android:textSize="42sp"
        app:layout_constraintBottom_toBottomOf="@+id/fitChart"
        app:layout_constraintEnd_toEndOf="@+id/fitChart"
        app:layout_constraintStart_toStartOf="@+id/fitChart"
        app:layout_constraintTop_toTopOf="@+id/fitChart" />

    <com.txusballesteros.widgets.FitChart
        android:id="@+id/fitChart3"
        android:layout_width="38dp"
        android:layout_height="96dp"
        android:layout_marginTop="88dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fitChart"
        app:layout_constraintVertical_bias="0.164"
        app:strokeSize="15dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="17"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/fitChart3"
        app:layout_constraintEnd_toEndOf="@+id/fitChart3"
        app:layout_constraintStart_toStartOf="@+id/fitChart3"
        app:layout_constraintTop_toTopOf="@+id/fitChart3" />

    <com.txusballesteros.widgets.FitChart
        android:id="@+id/fitChart2"
        android:layout_width="38dp"
        android:layout_height="96dp"
        android:layout_marginTop="88dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/fitChart3"
        app:layout_constraintTop_toBottomOf="@+id/fitChart"
        app:layout_constraintVertical_bias="0.164"
        app:strokeSize="15dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="17"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/fitChart2"
        app:layout_constraintEnd_toEndOf="@+id/fitChart2"
        app:layout_constraintStart_toStartOf="@+id/fitChart2"
        app:layout_constraintTop_toTopOf="@+id/fitChart2" />

    <TextView
        android:id="@+id/fname2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:fontFamily="@font/poppins"
        android:text="Josh"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/fitChart3"
        app:layout_constraintEnd_toEndOf="@+id/fitChart3"
        app:layout_constraintStart_toStartOf="@+id/fitChart3" />

    <TextView
        android:id="@+id/fname1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:fontFamily="@font/poppins"
        android:text="Alexander"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/fitChart1"
        app:layout_constraintEnd_toEndOf="@+id/fitChart1"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toStartOf="@+id/fitChart1" />

    <com.txusballesteros.widgets.FitChart
        android:id="@+id/fitChart1"
        android:layout_width="38dp"
        android:layout_height="96dp"
        android:layout_marginTop="88dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/fitChart3"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fitChart"
        app:layout_constraintVertical_bias="0.164"
        app:strokeSize="15dp" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:text="17"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/fitChart1"
        app:layout_constraintEnd_toEndOf="@+id/fitChart1"
        app:layout_constraintStart_toStartOf="@+id/fitChart1"
        app:layout_constraintTop_toTopOf="@+id/fitChart1" />

    <TextView
        android:id="@+id/fname3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:fontFamily="@font/poppins"
        android:text="Mahmoud"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/fitChart2"
        app:layout_constraintEnd_toEndOf="@+id/fitChart2"
        app:layout_constraintStart_toStartOf="@+id/fitChart2" />

    <TextView
        android:id="@+id/UserName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginLeft="22dp"
        android:fontFamily="@font/poppins"
        android:text="Welcome back, Alex!"
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/fitChart"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.387" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="67dp"
        android:layout_height="56dp"
        android:src="@drawable/avatar"
        app:layout_constraintBottom_toTopOf="@+id/fitChart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.838"
        app:layout_constraintStart_toEndOf="@+id/UserName"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.355" />

</androidx.constraintlayout.widget.ConstraintLayout>
